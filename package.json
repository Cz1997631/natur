		"@types/react-dom": "^16.8.4",
		"babel-loader": "^8.0.6",
		"html-webpack-plugin": "3.2.0",
		"rimraf": "^2.6.3",
		"typescript": "3.5.3",
		"webpack": "4.39.0",
		"webpack-cli": "^3.3.2",
		"webpack-dev-server": "^3.3.1"
	},
	"dependencies": {
		"hoist-non-react-statics": "^3.3.0",
